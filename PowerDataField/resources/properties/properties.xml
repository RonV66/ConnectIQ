<resources>
    <properties>
        <property id="ftp" type="number">200</property>
        <property id="powerGemiddeldeTijd" type="number">3</property>
        <property id="displayBar" type="number">0</property>
        <property id="display" type="number">0</property>
    </properties>
    
    <settings>
        <setting propertyKey="@Properties.ftp" title="@Strings.ftp">
            <settingConfig type="numeric" errorMessage="@Strings.errorFtp" min="10" max="4000" required="true"/>
        </setting>
        <setting propertyKey="@Properties.powerGemiddeldeTijd" title="@Strings.powerGemiddeldeTijd">
            <settingConfig type="list">
                <listEntry value="0">@Strings.sec0</listEntry>
                <listEntry value="3">@Strings.sec3</listEntry>
                <listEntry value="5">@Strings.sec5</listEntry>
                <listEntry value="10">@Strings.sec10</listEntry>
                <listEntry value="30">@Strings.sec30</listEntry>
            </settingConfig>
        </setting>
        <setting propertyKey="@Properties.displayBar" title="@Strings.displayBar">
            <settingConfig type="list">
                <listEntry value="0">@Strings.displayBarNee</listEntry>
                <listEntry value="1">@Strings.displayBarJa</listEntry>
            </settingConfig>
        </setting>
        <setting propertyKey="@Properties.display" title="@Strings.display">
            <settingConfig type="list">
                <listEntry value="0">@Strings.displayWatts</listEntry>
                <listEntry value="1">@Strings.displayWperKG</listEntry>
            </settingConfig>
        </setting>
    </settings>
</resources>
